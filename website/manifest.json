{  
	"name": "chrome网址导航",  
  	"version": "1.0.0",  
  	"description": "chrome网址导航--最实用的上网导航。及时收录包括视频、新闻、购物、体育、团购等各种分类的优秀网站，提供最简单便捷的网上导航服务。",  
  	"browser_action": {  
		"default_icon": "icon.png",
		"popup": "popup.html"
  	},
	"content_scripts": [ {
		"all_frames": true,
		"matches": ["https://*/*", "http://*/*"],
		"js": ["scripts/kissy-min.js", "scripts/keymaster-min.js", "scripts/webFunction.js"],
		"css": ["styles/style-pop.css"],
		"run_at": "document_end"
	} ],
  	"icons":{
		"48":"icon48x48.png"
	}	
}  
